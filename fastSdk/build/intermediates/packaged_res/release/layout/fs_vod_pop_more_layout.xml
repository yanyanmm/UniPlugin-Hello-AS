<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="@drawable/fs_pop_more_bg"
    android:orientation="vertical" >
    <LinearLayout
        android:id="@+id/txtShare"
        android:visibility="gone"
        style="@style/fs_pop_more_item_ly" >

        <TextView
            style="@style/fs_pop_more_style_tv"
            android:drawableLeft="@drawable/fs_icon_more_share"
            android:text="@string/fs_share"/>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/txtDiagnose"
        style="@style/fs_pop_more_item_ly" >

        <TextView
            style="@style/fs_pop_more_style_tv"
            android:drawableLeft="@drawable/fs_icon_more_diagnos"
            android:text="@string/fs_diagnosis"/>

    </LinearLayout>
</LinearLayout>